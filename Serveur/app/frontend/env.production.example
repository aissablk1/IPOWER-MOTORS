# Configuration de production pour le frontend IPOWER MOTORS
# Copier vers .env.production et remplir les vraies valeurs

# API Backend
VITE_API_URL=https://api.ipowerfrance.fr
VITE_API_VERSION=v1

# CDN et ressources statiques
VITE_CDN_URL=https://cdn.ipowerfrance.fr
VITE_S3_BUCKET=ipower-motors-frontend

# Configuration de l'application
VITE_APP_NAME=IPOWER MOTORS
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=production

# Services externes
VITE_GOOGLE_ANALYTICS_ID=GA_MEASUREMENT_ID
VITE_GOOGLE_MAPS_API_KEY=YOUR_GOOGLE_MAPS_API_KEY

# Configuration des fonctionnalités
VITE_ENABLE_PWA=true
VITE_ENABLE_OFFLINE=true
VITE_ENABLE_NOTIFICATIONS=true

# URLs de redirection
VITE_LOGIN_URL=https://admin.ipowerfrance.fr/login
VITE_LOGOUT_URL=https://admin.ipowerfrance.fr/logout
VITE_REGISTER_URL=https://admin.ipowerfrance.fr/register

# Configuration du cache
VITE_CACHE_VERSION=1.0.0
VITE_CACHE_DURATION=3600

# Configuration des images
VITE_IMAGE_QUALITY=80
VITE_IMAGE_FORMATS=webp,avif,jpg
VITE_IMAGE_SIZES=300,600,900,1200

# Configuration des polices
VITE_FONT_PRELOAD=true
VITE_FONT_DISPLAY=swap

# Configuration des métriques
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_ERROR_TRACKING=true
